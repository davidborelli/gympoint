{
  "name": "youch",
  "version": "2.0.10",
  "description": "HTML Pretty error stack viewer",
  "main": "src/Youch.js",
  "files": [
    "src"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "pretest": "npm run lint",
    "prepublishOnly": "npm run build",
    "build": "node bin/compile.js",
    "test": "npm run build && node test/youch.spec.js",
    "lint": "standard"
  },
  "author": "amanvirk",
  "license": "MIT",
  "devDependencies": {
    "concat": "^1.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^2.0.3",
    "standard": "^12.0.1",
    "supertest": "^3.3.0",
    "uglify-js": "^3.4.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "mustache": "^3.0.0",
    "stack-trace": "0.0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/youch.git"
  },
  "keywords": [
    "errors",
